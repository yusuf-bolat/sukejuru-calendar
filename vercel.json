{
  "version": 2,
  "name": "sukejuru-calendar",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
